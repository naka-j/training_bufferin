<!DOCTYPE html>
<html>
  <head>
    <title>TrainingBufferin</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="header">
      <div class="container">
        <%= render 'layouts/header' %>
      </div>
    </div>
    <div class="main">
      <div class="container">
        <%= render 'shared/notices' %>
        <%= yield %>
      </div>
    </div>
    <div class="footer">
      <div class="container">
        <%= render 'layouts/footer' %>
      </div>
    </div>
  </body>
</html>
